<!DOCTYPE softpkg PUBLIC "-//JTRS//DTD SCA V2.2.2 SPD//EN" "softpkg.dtd">
<softpkg id="DCE:fbc168ff-6db2-4c74-ac24-b3380f45c7ff" name="FMDemodulator" type="sca_compliant">
  <title></title>
  <author>
    <name>null</name>
  </author>
  <description>This component performs an FM demodulation using the liquid-dsp library (http://liquidsdr.org). This component takes in complex data that is at baseband.</description>
  <propertyfile type="PRF">
    <localfile name="FMDemodulator.prf.xml"/>
  </propertyfile>
  <descriptor>
    <localfile name="FMDemodulator.scd.xml"/>
  </descriptor>
  <implementation id="cpp">
    <description>The implementation contains descriptive information about the template for a software component.</description>
    <code type="Executable">
      <localfile name="cpp/FMDemodulator"/>
      <entrypoint>cpp/FMDemodulator</entrypoint>
    </code>
    <compiler name="/usr/bin/gcc" version="4.1.2"/>
    <programminglanguage name="C++"/>
    <humanlanguage name="EN"/>
    <os name="Linux"/>
    <dependency type="runtime_requirements">
      <softpkgref>
        <localfile name="/sharedPkgs/liquid-dsp/libliquid.spd.xml"/>
        <implref refid="default_impl"/>
      </softpkgref>
    </dependency>
    <processor name="x86"/>
    <processor name="x86_64"/>
  </implementation>
  <implementation id="cpp_arm">
    <description>The implementation contains descriptive information about the template for a software component.</description>
    <code type="Executable">
      <localfile name="cpp_arm/FMDemodulator"/>
      <entrypoint>cpp_arm/FMDemodulator</entrypoint>
    </code>
    <compiler name="/usr/bin/gcc" version="4.1.2"/>
    <programminglanguage name="C++"/>
    <humanlanguage name="EN"/>
    <os name="Linux"/>
    <dependency type="runtime_requirements">
      <softpkgref>
        <localfile name="/sharedPkgs/liquid-dsp/libliquid.spd.xml"/>
        <implref refid="default_impl"/>
      </softpkgref>
    </dependency>
    <processor name="armv7l"/>
  </implementation>
</softpkg>
